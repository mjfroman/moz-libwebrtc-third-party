import("//build/config/chrome_build.gni")
declare_args() {
  # This enables building WebNN with XNNPACK. It is currently only available on
  # x64 CPU because XNNPACK is only built for that.
  # See //third_party/xnnpack/BUILD.gn
  build_webnn_with_xnnpack =
      (is_win || is_linux || is_chromeos || is_android) && current_cpu == "x64"
}
