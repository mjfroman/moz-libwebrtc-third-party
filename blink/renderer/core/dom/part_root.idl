[RuntimeEnabled=DOMPartsAPI,Exposed=Window]
interface PartRoot {
  FrozenArray<Part> getParts();
  // This clones the PartRoot, along with the entire RootContainer tree it
  // points to, and returns the cloned PartRoot.
  PartRoot clone();
};
