import("//build_overrides/ipcz.gni")
import("//testing/test.gni")

test("ipcz_tests") {
  sources = [ "test/run_all_tests.cc" ]

  deps = [ "//testing/gtest" ]

  if (build_ipcz_standalone) {
    defines = [ "IPCZ_STANDALONE" ]
  }
}

group("all") {
  testonly = true
  deps = [ ":ipcz_tests" ]
}
